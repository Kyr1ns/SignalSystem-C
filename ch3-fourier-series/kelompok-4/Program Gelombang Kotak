import numpy as np
import matplotlib.pyplot as plt
from scipy.signal import square

amp = 1
freq = 5
dur = 1
fs = 1000

t = np.arange(0, dur, 1/fs)
sq = amp * square(2 * np.pi * freq * t)

plt.figure(figsize=(8,4))
plt.plot(t, sq, color='blue')
plt.title('Square Wave')
plt.xlabel('Time (s)')
plt.ylabel('Amplitude')
plt.grid(True)
plt.show()
